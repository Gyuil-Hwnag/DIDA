<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_login"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.dida.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_nicknameFragment"
            app:popUpTo="@id/navigation_login"
            app:popUpToInclusive="true"
            app:destination="@id/nicknameFragment" />
    </fragment>

    <fragment
        android:id="@+id/nicknameFragment"
        android:name="com.dida.nickname.NicknameFragment"
        android:label="NicknameFragment">
        <argument
            android:name="email"
            app:argType="string"/>
    </fragment>
</navigation>
